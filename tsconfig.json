{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true
  },
  "exclude": ["node_modules"],
  "include": ["./src/*.tsx", "./cypress/integration/*.js"]
}
